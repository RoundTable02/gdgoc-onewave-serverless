# Server Configuration
PORT=8080
NODE_ENV=production

# Google Gemini AI
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=AIza...your-api-key

# Google Cloud Platform
GCS_BUCKET=connectable-grading-evidence
GCS_PROJECT_ID=your-project-id

# Grading Configuration
GRADING_TIMEOUT_MS=300000          # 전체 채점 타임아웃 (5분, 밀리초)
MAX_CONCURRENT_TESTS=5             # 동시 실행 최대 테스트 수
TEST_TIMEOUT_MS=30000              # 개별 테스트 타임아웃 (30초, 밀리초)
ENABLE_PARALLEL_EXECUTION=true     # 병렬 실행 활성화 (false로 설정 시 순차 실행)

# Browser Configuration
BROWSER_HEADLESS=true
ENABLE_VIDEO_RECORDING=false
